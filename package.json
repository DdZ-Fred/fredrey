  },
  "homepage": "https://github.com/DdZ-Fred/fredrey#readme",
  "dependencies": {
    "axios": "^0.11.1",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "nodemailer": "^2.4.1",
    "react": "^15.0.2",
